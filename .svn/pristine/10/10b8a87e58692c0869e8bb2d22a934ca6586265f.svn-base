.dataField {
	background: #F9F9F9; /* Old browsers */
	background: -moz-linear-gradient(#F9F9F9, #EEEEEE) repeat scroll 0 0 #F9F9F9; /* FF3.6+ */
	background: -webkit-gradient(linear, left top, right top, color-stop(0%,#F9F9F9), color-stop(100%,#EEEEEE)); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(left,  #F9F9F9 0%,#EEEEEE 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(left,  #F9F9F9 0%,#EEEEEE 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(left,  #F9F9F9 0%,#EEEEEE 100%); /* IE10+ */
	background: linear-gradient(to right,  #F9F9F9 0%,#EEEEEE 100%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#F9F9F9', endColorstr='#EEEEEE',GradientType=1 ); /* IE6-9 */	
	width:100%;
}

.dataField{
    font-family: 'BryantWebRegular';  
	/*-moz-box-shadow: 0 1px 4px rgba(0, 0, 0, 0.2) inset;
	-webkit-box-shadow: 0 1px 4px rgba(0, 0, 0, 0.2) inset;
	box-shadow: 0 1px 4px rgba(0, 0, 0, 0.2) inset;*/
	/*-moz-border-radius: 5px 5px 5px 5px;*/
	border-radius: 0.6em 0.6em 0.6em 0.6em;
	padding:2px; 	
	color: #696969;
	border: 1px solid #AAAAAA;	
}

