/*main css page*/

html, body {
	height: 100%;
	width: 100%;
	margin: 0;
	padding: 0;
	background-color: #6699CC;
}


@font-face {
	font-family: BryantWebRegular;
	src: 
		url('http://http://23.22.93.5/fonts/Bryant2Combo/Bryant2/Bryant-Regular.otf'), 
		url('http://http://23.22.93.5/fonts/Bryant2Combo/Bryant2Web_EOT/BryantWebRegular.eot');

}

@font-face {
	font-family: BryantWebMedium;
	src: 
		url('http://23.22.93.5/fonts/Bryant2Combo/Bryant2/Bryant-Medium.otf'), 
		url('http://23.22.93.5/fonts/Bryant2Combo/Bryant2Web_EOT/BryantWebMedium.eot');
}

@font-face {
	font-family: BryantWebBold;
	src: 
		url('http://23.22.93.5/fonts/Bryant2Combo/Bryant2/Bryant-Bold.otf'), 
		url('http://23.22.93.5/fonts/Bryant2Combo/Bryant2Web_EOT/BryantWebBold.eot');
}


#map {
	border-width: 1px;
	border-style: solid;
	border-color: #000;
	padding: 0;
	z-index: 0;
	height: 100%;
	width: 100%;
}

#header {
	top: 0px;
	left: 0px;
	right: 0px;
	height: 65px;
	background-color: #59595C;
	color: #E7E4CE;
	position: absolute;
	z-index: 1;
	display: inline;
	font-family: BryantWebRegular, "Lucida Sans Unicode", "Lucida Grande", sans-serif ;
	font-size: 40px;
}

#usgsLogoDiv {
	float: left;
	background-color: Black;
	height: 50px;
	width: 150px;
	padding-top: 10px;
	padding-right: 5px;
	padding-bottom: 5px;
	text-align: center;
}

#title {
	float: left;
	padding-right: 5px;
	padding-left: 10px;
	padding-top: 10px;
	font-family: "BryantWebBold","Lucida Sans Unicode", "Lucida Grande", sans-serif ;
}

#titleSeparator {
	color: #F37A49;
	float: left;
	padding-right: 5px;
	padding-top: 10px;
	font-family: BryantWebBold, "Lucida Sans Unicode", "Lucida Grande", sans-serif ;
}

#subTitle {
	padding-top: 10px;
}

#betaTitle {
	float: right;
	padding: 20px;
	font-size: 16px;
	color: #AAAAB5;
}

#geocodeBar {
	position: absolute;
	left: 5px;
	/* Fallback for web browsers that doesn't support RGBa */
	background: rgb(126, 126, 132);
	/* RGBa with 0.55 opacity */
	background: rgba(126, 126, 132, 1.0);
	/* For IE 5.5 - 7*/
	/*filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#7E7E8400, endColorstr=#7E7E8400);*/
	/* For IE 8*/
	/*-ms-filter: "progid:DXImageTransform.Microsoft.gradient(startColorstr=#7E7E8400, endColorstr=#7E7E8400)";*/
	z-index: 1;
	border: 1px solid #59595C;
	border-radius: 5px;
	-moz-border-radius: 5px;
	padding: 5px;
	display: inline;
	float: right;
	top: 150px;
}

#geocodeBar #geocode {
	width: 140px;
	height: 15px;
	font-size: 12px;
	font-style: italic;
}

#geocodeBar #searchIcon {
	width: 15px;
	height: 15px;
	vertical-align: middle;
	/*overrides background color set for all input tags below*/
	background:#7E7E84;
}

.searchButtons, .clearButtons {
	height: 20px;
	width: 85px;
	color: white;
	font-family: BryantWebMedium, "Lucinda Sans Unicode", "Lucinda Grande", sans-serif;
	/*letter-spacing: 1px;*/
	font-size:12px;
	border-radius: 5px;
	-moz-border-radius:5px;
	border: 0px;
	cursor: pointer;
}

.searchButtons:hover, .clearButtons:hover{
	box-shadow:0 0 5px #e7e4ce;
}

.searchButtons{
	background-color: rgb(243, 122, 73);
}

.clearButtons{
	background-color: rgb(89,89,92);
}

#wimLogo {
	position: absolute;
	bottom: 5px;
	right: 5px;
	z-index: 40;
}

#usgsLogo {
	height: 45px;
	/*vertical-align: middle;*/
	cursor: pointer;
}

#usgsLinks {
	font-family: BryantWebRegular;
	margin-top: 0px;
	position: absolute;
	padding-top: 0px;
	padding-left: 5px;
	padding-right: 5px;
	height: 285px;
	width: 135px;
	z-index: 5;
	background-color: gray;
	line-height: 1.75;
	border-radius: 5px;
	-moz-border-radius: 5px;
	box-shadow: 0 0 .25em #000;
}

.usgsLinksHeader {
	top: 0px;
	font-size: 18px;
	height: 40px;
	width: 98%;
	color: white;
	text-align: center;
	border: 1px solid #B5BCC7;
	background-color: black;
	border-radius: 5px;
	-moz-border-radius: 5px;
	box-shadow: 0 0 0.25em #000;
}

#loadingStatus {
	position: absolute;
	right: 49%;
	top: 48%;
	z-index: 99;
}

#loadingStatusContent {
	background-color: Black;
	color: White;
	border-radius: 5px;
	-moz-border-radius: 5px;
	padding: 10px;
	display: inline;
	width: 135px;
}


#searchContainer {
	position:absolute;
	height: 80px;
	top: 65px;
	right: 0px;
	z-index: 1;
	display: block;
	/*displays gray box behind search controls*/
	/*background-color: rgb(89,89,92);
	border: 1px solid #B5BCC7;*/
}

#leftSideContainers {
	position: absolute;
	top:190px;
	left:0px;
	z-index:2;
	display:inline;
	margin:5px;
}

#availableLayersLeftSide {
	z-index: 2;
	left: 0px;
	margin-bottom: 10px;
	border-top-right-radius: 5px;
	-moz-border-top-right-radius: 5px;
	-webkit-border-top-right-radius: 5px;
	border-bottom-right-radius:5px;
	-moz-border-bottom-right-radius: 5px;
	-webkit-border-bottom-right-radius: 5px;
	width: 270px;
}

#tabs{
	/*container width/height set in index.html*/
	margin-bottom:10px;
	z-index:2;
	right:0px;
	border-top-left-radius: 5px;
	-moz-border-top-left-radius: 5px;
	-webkit-border-top-left-radius: 5px;
	border-bottom-left-radius: 5px;
	-moz-border-bottom-left-radius: 5px;
	-webkit-border-bottom-left-radius: 5px;	
}

#spacer {
	/*height: 10px;*/
}

#explanationLeftSide {
	margin-bottom:10px;
	position: absolute;
	left: 0px;
	z-index: 2;
	width: 270px;
	/*rounds border radius on right side*/
	border-top-right-radius: 5px;
	-moz-border-top-right-radius: 5px;
	-webkit-border-top-right-radius: 5px;
	border-bottom-right-radius: 5px;
	-moz-border-bottom-right-radius: 5px;
	-webkit-border-bottom-right-radius: 5px;
}

#layersElements {
	font-size: 22px;
	font-weight: normal;
	opacity: 1.0;
	filter: alpha(opacity=100);
	padding-bottom: 10px;
}

#legend {
	font-family: BryantWebRegular;
	background-color: #E7E4CE;
	color: Black;
	font-size: 14px;
	font-weight: normal;
	padding: 5px;
	max-height: 400px;
	overflow: auto;
	border-bottom-right-radius: 5px;
	/*use for left side containers*/
	-moz-border-bottom-right-radius:5px;
	-webkit-border-bottom-right-radius:5px;
}

#mobileLayers {
	color: Black;
	visibility: hidden;
	position: absolute;
	bottom: 5px;
	right: 5px;
	background-color: Black;
	border-style: solid;
	border-width: 1;
	border-color: Black;
	color: White;
	z-index: 1;
	padding: 2px;
	display: inline;
}

#backToMap {
	visibility: hidden;
	float: right;
}

#infoWindow {
	position: absolute;
	top: 0px;
	left: 0px;
	height: 50px;
	width: 50px;
	background-color: gray;
	visibility: hidden;
}

