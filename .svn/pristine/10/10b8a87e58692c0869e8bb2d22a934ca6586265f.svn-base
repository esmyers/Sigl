.esriSimpleSliderVertical   {
  -webkit-border-radius: 5px;
  -moz-border-radius: 5px;
  border-radius: 5px;
  background-color:#59595C;
  border: 1px solid #B5BCC7;
  
}

.esriSimpleSliderIncrementButton {
	border-top-left-radius: 5px;
	-moz-border-top-left-radius: 5px;
	-webkit-border-top-left-radius: 5px;
	border-top-right-radius: 5px;
	-moz-border-top-right-radius: 5px;
	-webkit-border-top-right-radius: 5px;
	width:25px;
	height: 25px;
	border: none !important;
	opacity:1;
	
}

.esriSimpleSliderDecrementButton {
	border-bottom-left-radius: 5px;
	-moz-border-bottom-left-radius: 5px;
	-webkit-border-bottom-left-radius: 5px;
	border-bottom-right-radius: 5px;
	-moz-border-bottom-right-radius: 5px;
	-webkit-border-bottom-right-radius: 5px;
	width:25px;
	height: 25px;
	border:none; !important;
	opacity:1;
}

.esriSimpleSlider {
  top: 95px;
  left:10px;
  border: none !important;
  color:#fff !important;
  box-shadow:none !important;
  -webkit-box-shadow:none;
  -moz-box-shadow:none;
  opacity: 0.7 !important;
  filter: alpha(opacity=70) !important;
  background-color: #59595C !important;
  border-radius:none !important;
}

.esriSimpleSlider div{
	-moz-user-select: none;
    font-family: BryantWebBold,verdana,helvetica;
    font-size: 24px;
    font-weight: bold;
    height: 25px;
    line-height: 25px;
    width: 25px;
  
}

.esriLargeSliderVertical .esriLargeSliderIncrementButton, .esriLargeSliderVertical .esriLargeSliderDecrementButton {
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px;
	background-color: #59595C;
	color:#59595C; 
}

.esriLargeSlider {
	top: 95px;
	left: 5px;
	background-color: #59595C; !important;
	padding-top:2px;
	padding-left: 2px;
	padding-bottom:2px;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px;
	color: #59595C; !important;
	opacity:0.7;
}

.dijitRuleMark {
	border: 1px solid white; !important
}

