	background-color: #F37A49;
	padding: 2px;
	border: 0px;
	font-family: BryantWebRegular, "Lucida Sans Unicode", "Lucida Grande", sans-serif ;
	border-radius: 0px;
	-moz-border-radius: 0px;
	border-bottom-left-radius: 5px;
	border-bottom-right-radius: 5px;
	-moz-border-bottom-left-radius: 5px;
	-moz-border-bottom-right-radius: 5px;
}

#basemapSelector .dijitTitlePaneContentOuter {
	border-bottom-left-radius: 5px;
	border-bottom-right-radius: 5px;
	-moz-border-bottom-left-radius: 5px;
	-moz-border-bottom-right-radius: 5px;
}

#basemapSelector {
	font-weight: 500;
	font-size: 16px;
	color: White;
	border-bottom-left-radius: 5px;
	border-bottom-right-radius: 5px;
	-moz-border-bottom-left-radius: 5px;
	-moz-border-bottom-right-radius: 5px;
	font-family: BryantWebRegular, "Lucida Sans Unicode", "Lucida Grande", sans-serif ;
	position: absolute;
	right: 275px;
